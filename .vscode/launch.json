    {
      "type": "lldb",
      "request": "launch",
      "name": "1. test mirrord (lldb)",
      "cargo": {
        "args": [
          "test", "file_ops_tests", "--manifest-path", "tests/Cargo.toml", "--no-run",
          "--target", "x86_64-pc-windows-msvc"
        ],
        "env": {
          "MIRRORD_LAYER_FILE": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/layer_win.dll",
          "RUST_BACKTRACE": "1",
          "MIRRORD_TESTS_USE_BINARY": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/mirrord.exe",
        },
        "filter": {
          "name": "tests"
        },
      },
      "cwd": "${workspaceFolder}/tests",
      // "program": "target/x86_64-pc-windows-msvc/debug/deps/tests-08b24540faf8040c.exe",
      "args": ["--nocapture"],
      "env": {
        "MIRRORD_LAYER_FILE": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/layer_win.dll",
      },
      "console": "integratedTerminal",
      "preLaunchTask": "cargo: build mirrord (msvc)",
      "sourceMap": {},
      "sourceLanguages": [
        "rust"
      ],
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "2. debug cli exec",
      "cargo": {
        "args": [
          "build", "-p", "mirrord", "--target", "x86_64-pc-windows-msvc" // "--release"
        ],
        "filter": {
          "name": "mirrord"
        },
        "env": {
          "MIRRORD_LAYER_FILE": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/layer_win.dll",
          // "RUSTFLAGS": "-LC:/Users/Daniel/git/mirrord/target/x86_64-pc-windows-msvc/release/build/aws-lc-sys-f222e66db1014569/out/build/artifacts",
        },
