      "args": [
        "exec", "-c", "--target", "pod/http-echo-omv4wcp-55949b95bb-2jvsc/container/test", "--target-namespace", "default", "--fs-mode", "read", "-e", "--", "../target/x86_64-pc-windows-msvc/debug/rust-e2e-fileops"
      ],
      "program": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/mirrord.exe",
      "env": {
        "MIRRORD_DISABLE_VERSION_CHECK": "true",
        "MIRRORD_LAYER_FILE": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/layer_win.dll",
        "MIRRORD_OPERATOR_ENABLE": "false",
